{
  "name": "mi-service-lite",
  "version": "1.0.0",
  "type": "module",
  "description": "XiaoMi Cloud Service for mi.com This is a fork from https://github.com/inu1255/mi-service",
  "license": "MIT",
  "author": {
    "name": "Del Wang",
    "email": "hello@xbox.work",
    "url": "https://github.com/idootop"
  },
  "keywords": [
    "mi",
    "xiaomi",
    "mina",
    "miiot",
    "mi-service"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc --module CommonJS && cd dist && mv \"index.js\" \"index.cjs\" && cd ../ && tsc --module esnext"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.9",
    "@types/pako": "^2.0.3",
    "dotenv": "^16.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "default": "./dist/index.js"
  },
  "homepage": "https://github.com/idootop/mi-service-lite",
  "bugs": {
    "url": "https://github.com/idootop/mi-service-lite/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idootop/mi-service-lite.git"
  }
}
